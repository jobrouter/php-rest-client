{
    "name": "brotkrueml/jobrouter-client",
    "type": "library",
    "description": "Rest client for the JobRouter® digitalisation platform",
    "keywords": [
        "jobrouter",
        "rest client"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Müller",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.3",
        "symfony/polyfill-php80": "^1.18"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~1.1.0",
        "captainhook/captainhook": "^5.10",
        "donatj/mock-webserver": "dev-php81",
        "ergebnis/composer-normalize": "^2.15",
        "infection/infection": "^0.25",
        "mikey179/vfsstream": "^1.6.10",
        "php-coveralls/php-coveralls": "^2.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.11.60",
        "symplify/phpstan-rules": "^9.3"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\JobRouterClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\JobRouterClient\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/brotkrueml/mock-webserver"
        }
    ],
    "support": {
        "issues": "https://github.com/brotkrueml/jobrouter-client/issues",
        "source": "https://github.com/brotkrueml/jobrouter-client",
        "docs": "https://jobrouter-client.readthedocs.io/"
    }
}
