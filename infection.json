{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "timeout": 10,
    "source": {
        "directories": [
            "src"
        ]
    },
    "phpUnit": {
        "configDir": "tests"
    },
    "logs": {
        "stryker": {
            "report": "main"
        },
        "html": "logs/infection.html",
        "text": "logs/infection.log"
    },
    "mutators": {
        "@default": true,
        "DecrementInteger": {
            "ignore": [
                "Brotkrueml\\JobRouterClient\\Client\\RestClient::authenticate::111"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "Brotkrueml\\JobRouterClient\\Client\\RestClient::authenticate::111"
            ]
        }
    }
}
